<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="mappers.BusinessType">
     
    <select id="selectAll" resultType="CommentVO">
  		SELECT id, name WHERE business_type;
 	</select>
 
    <select id="selectById" resultType="CommentVO">
  		SELECT id, name WHERE business_type WHERE id=#{id};
 	</select>
  
     <select id="selectCount" resultType="CommentVO">
  		SELECT cOUNT(*) WHERE business_type GROUP BY id;
	 </select>
     
 </mapper>